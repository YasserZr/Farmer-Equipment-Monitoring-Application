{
  "dashboard": {
    "id": null,
    "uid": "farm-monitoring-overview",
    "title": "Farm Equipment Monitoring - Overview",
    "tags": ["microservices", "spring-boot", "overview"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "Service Health Status",
        "targets": [
          {
            "expr": "up{job=~\".*-service\"}",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "min": 0, "max": 1},
          {"format": "short"}
        ],
        "legend": {"show": true, "values": false, "alignAsTable": true},
        "nullPointMode": "null"
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph",
        "title": "Request Rate (req/s)",
        "targets": [
          {
            "expr": "sum(rate(http_server_requests_seconds_count[5m])) by (service)",
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps"},
          {"format": "short"}
        ],
        "legend": {"show": true, "values": false, "alignAsTable": true}
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "type": "graph",
        "title": "Response Time (P95)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket[5m])) by (service, le))",
            "legendFormat": "{{service}} - p95",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s"},
          {"format": "short"}
        ],
        "legend": {"show": true, "values": true, "current": true, "alignAsTable": true}
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "type": "graph",
        "title": "Error Rate (%)",
        "targets": [
          {
            "expr": "sum(rate(http_server_requests_seconds_count{status=~\"5..\"}[5m])) by (service) / sum(rate(http_server_requests_seconds_count[5m])) by (service) * 100",
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "min": 0},
          {"format": "short"}
        ],
        "legend": {"show": true, "values": true, "current": true, "alignAsTable": true}
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16},
        "type": "graph",
        "title": "JVM Heap Memory Usage",
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{area=\"heap\"} / jvm_memory_max_bytes{area=\"heap\"} * 100",
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "min": 0, "max": 100},
          {"format": "short"}
        ],
        "legend": {"show": true, "values": true, "current": true, "alignAsTable": true}
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16},
        "type": "graph",
        "title": "CPU Usage",
        "targets": [
          {
            "expr": "process_cpu_usage * 100",
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "min": 0, "max": 100},
          {"format": "short"}
        ],
        "legend": {"show": true, "values": true, "current": true, "alignAsTable": true}
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16},
        "type": "graph",
        "title": "Thread Count",
        "targets": [
          {
            "expr": "jvm_threads_live_threads",
            "legendFormat": "{{service}} - live",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short"},
          {"format": "short"}
        ],
        "legend": {"show": true, "values": true, "current": true, "alignAsTable": true}
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "type": "graph",
        "title": "Database Connection Pool",
        "targets": [
          {
            "expr": "hikaricp_connections_active",
            "legendFormat": "{{service}} - active",
            "refId": "A"
          },
          {
            "expr": "hikaricp_connections_idle",
            "legendFormat": "{{service}} - idle",
            "refId": "B"
          },
          {
            "expr": "hikaricp_connections_max",
            "legendFormat": "{{service}} - max",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "short"},
          {"format": "short"}
        ],
        "legend": {"show": true, "values": true, "current": true, "alignAsTable": true}
      },
      {
        "id": 9,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "type": "graph",
        "title": "RabbitMQ Queue Depth",
        "targets": [
          {
            "expr": "rabbitmq_queue_messages",
            "legendFormat": "{{queue}} - ready",
            "refId": "A"
          },
          {
            "expr": "rabbitmq_queue_messages_unacknowledged",
            "legendFormat": "{{queue}} - unacked",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short"},
          {"format": "short"}
        ],
        "legend": {"show": true, "values": true, "current": true, "alignAsTable": true}
      },
      {
        "id": 10,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "type": "graph",
        "title": "Garbage Collection Time",
        "targets": [
          {
            "expr": "rate(jvm_gc_pause_seconds_sum[5m])",
            "legendFormat": "{{service}} - {{action}}/{{cause}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s"},
          {"format": "short"}
        ],
        "legend": {"show": true, "values": true, "current": true, "alignAsTable": true}
      },
      {
        "id": 11,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "type": "graph",
        "title": "Equipment Status Distribution",
        "targets": [
          {
            "expr": "sum(equipment_status_total) by (status)",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short"},
          {"format": "short"}
        ],
        "legend": {"show": true, "values": true, "current": true, "alignAsTable": true}
      }
    ],
    "templating": {
      "list": [
        {
          "name": "datasource",
          "type": "datasource",
          "query": "prometheus"
        },
        {
          "name": "service",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(up{job=~\".*-service\"}, job)",
          "multi": true,
          "includeAll": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertstate=\"firing\"}",
          "name": "Alerts",
          "titleFormat": "Alert: {{alertname}}"
        }
      ]
    }
  },
  "folderId": 0,
  "overwrite": true
}
