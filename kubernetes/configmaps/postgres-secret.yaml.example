# Example Secret for PostgreSQL (Base64 encoded values)
# To create: kubectl create secret generic postgres-secret --from-literal=username=postgres --from-literal=password=postgres

apiVersion: v1
kind: Secret
metadata:
  name: postgres-secret
type: Opaque
data:
  # Default values (base64 encoded):
  # username: postgres -> cG9zdGdyZXM=
  # password: postgres -> cG9zdGdyZXM=
  username: cG9zdGdyZXM=
  password: cG9zdGdyZXM=
